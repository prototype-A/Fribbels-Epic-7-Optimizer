            "useLockedGearOption": "Locked items",
            "useEquippedGearOption": "Equipped items",
            "keepCurrentGearOption": "Keep current",
            "optionsExcludeGearFromDropdownPlaceholder": "Exclude equipped",

            "statFiltersLabel": "Stat Filters",
            "statFiltersTooltip": "<p>You can manually input (a range of) specific substat values to filter your optimization results by. Left column is min (inclusive) and right column is max (inclusive).</p>",
            "statAtkLabel": "Atk",
            "statHpLabel": "Hp",
